    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "live-server": "^1.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
